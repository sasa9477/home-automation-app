generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model SWitcher {
  id        Int     @id @default(autoincrement())
  ipaddress String  @unique
  name      String
  enabled   Boolean @default(false)
}

model Log {
  id        Int       @id @default(autoincrement())
  message   String
  createdAt DateTime  @default(now()) @map(name: "created_at")
}
